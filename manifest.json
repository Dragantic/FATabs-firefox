{
	"manifest_version": 2,
	"name": "FATabs",
	"version": "1.0",
	"description": "Download images from open FurAffinity image tabs",
	"homepage_url": "https://github.com/dragantic/fatabs",

	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"browser_style": true,
		"default_title": "FATabs",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"downloads",
		"*://*.furaffinity.net/view/*"
	]
}
